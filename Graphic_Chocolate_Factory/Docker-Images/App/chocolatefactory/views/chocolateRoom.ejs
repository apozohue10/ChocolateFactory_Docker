<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>Chocolate Room</title>
  <script src="http://localhost:1028/socket.io/socket.io.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/prefixfree/1.0.7/prefixfree.min.js"></script>  
  <link rel='stylesheet prefetch' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css'>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/meyer-reset/2.0/reset.min.css">

      <link rel="stylesheet" href="stylesheets/styleChocolateRoom.css">

  
</head>

<body>
  <h1>Chocolate Room</h1>
  <div class="logout"><a href="/back" onclick="Fiware.signOut('account'); return false">Go Back</a></div> 
  <div class="controls">
  <div id="speed">
    <span>Waterfall Speed</span>
      <div id="plusSpeed" class="btn btn-default">+</div>
      <div id="speedValue">0</div>
      <div id="minusSpeed" class="btn btn-default">-</div>
  </div>
  <div id="river"> 
    <span id="river">River Level</span>
      <div id="plusRiver" class="btn btn-default">+</div>
      <div id="riverValue">0</div>
      <div id="minusRiver" class="btn btn-default">-</div>
  </div>
  <div id="temp"> 
    <span id="press">Temperature</span>
      <div id="tempValue">0</div>
  </div>
  <div id="press"> 
    <span id="press">Pressure</span>
      <div id="pressValue">0</div>
  </div>
  </div>
  <div id="scene">
    <div id="container">
      <canvas id="waterfall"></canvas>
    </div>
  </div>

<script src='http://cdnjs.cloudflare.com/ajax/libs/gsap/1.18.2/TweenMax.min.js'></script>
<script src='http://cdnjs.cloudflare.com/ajax/libs/three.js/r75/three.min.js'></script>
<script src='http://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.6.1/lodash.min.js'></script>
<script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
    <script src="javascripts/waterfall_river.js"></script>
    <script src="javascripts/chocolateRoom.js"></script>
</body>
</html>
